{
  "name": "AIO-Static-Site-Generator",
  "version": "1.0.0",
  "description": "a profilr page of gh",
  "main": "index.js",
  "scripts": {
    "start": "npm run open && npm run start:server",
    "open": "start http://localhost:3000",
    "new": "npm run scaffolding:structure -s",
    "clean": "npm run removing:structure -s",
    "scaffolding:structure": "mkdirp _kitchen/css _kitchen/public/js && touch _kitchen/public/index.html",
    "removing:structure": "rimraf _kitchen",
    "start:server": "nodemon server.js",
    "pug:compile": "pug --obj ./site-config.json ./src --pretty --out ./dist",
    "pug:watch": "pug ./site-config.json ./src --watch",
    "less:compile": "lessc ./src ./dist"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "concurrently": "^3.1.0",
    "express": "^4.14.0",
    "html2jade": "^0.8.6",
    "jstransformer-markdown-it": "^2.0.0",
    "less": "^2.7.1",
    "less-middleware": "^2.2.0",
    "mkdirp": "^0.5.1",
    "pug": "^2.0.0-beta6",
    "rimraf": "^2.5.4",
    "watch": "^1.0.1"
  },
  "devDependencies": {
    "connect-livereload": "^0.6.0",
    "livereload": "^0.6.0",
    "markdown-it-decorate": "^1.2.1",
    "nodemon": "^1.11.0"
  }
}
